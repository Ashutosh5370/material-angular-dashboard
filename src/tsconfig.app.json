{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "experimentalDecorators": true,
    "allowJs": true,
    "types": ["d3", "nvd3", "material-design-lite"],
    "target": "es5",
    "downlevelIteration": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "files": [
    "main.ts",
    "polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
